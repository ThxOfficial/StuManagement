<!--pages/student/stuctr/stuctr.wxml-->
<view class="bg">
<text>学生端个人信息</text>
<view class='ctr'>
  <view class='img'>
<image src="{{pic}}"></image>
</view>
<view class='ttl'>姓名:</view>
<view class='ctt'>{{name}}</view>
<view class='ttl'>昵称:</view>
<view class='ctt'>{{nkname}}</view>
<view class='ttl'>性别:</view>
<view class='ctt'>{{sex}}</view>
<view class='ttl'>个性签名:</view>
<view class='ctt'>{{sig}}</view>
</view>
<button type="primary" bindtap='Change'>个人信息修改</button>
<button type="primary" bindtap='Exit'>退出登录</button>
<button bindtap='Dlusr' class='delbtn'>注销</button>
</view>
